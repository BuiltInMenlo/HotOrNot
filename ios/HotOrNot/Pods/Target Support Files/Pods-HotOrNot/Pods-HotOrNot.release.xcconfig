ENABLE_NS_ASSERTIONS = YES
FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/LayerKit/LayerKit.embeddedframework"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/LayerKit" "${PODS_ROOT}/Headers/Public/PubNub"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/LayerKit" -isystem "${PODS_ROOT}/Headers/Public/PubNub"
OTHER_LDFLAGS = -ObjC -l"Pods-HotOrNot-LayerKit" -l"Pods-HotOrNot-PubNub" -l"z" -framework "CFNetwork" -framework "LayerKit" -framework "MobileCoreServices" -framework "Security" -framework "SystemConfiguration"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods